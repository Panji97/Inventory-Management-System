<script>
  const logout = document.getElementById("logout");

  try {
    logout.addEventListener("click", async () => {
      const res = await fetch("/logout");
      if ((res.data.status = "success")) location.assign("/");
    });
  } catch (error) {
    console.log(error);
  }
</script>
